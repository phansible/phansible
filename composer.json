{
  "name": "phansible/phansible",
  "description": "Phansible - generate Vagrant + Ansible dev environments for PHP",
  "homepage": "http://phansible.com",
  "authors": [
    {
      "name": "Erika Heidi",
      "homepage": "http://erikaheidi.com"
    },
    {
      "name": "Marco \"Debo\" De Bortoli",
      "homepage": "https://debo.io"
    }
  ],
  "license": "MIT",
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "symfony/flex": true,
      "php-http/discovery": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "symfony/flex": "^2.4",
    "symfony/http-foundation": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/proxy-manager-bridge": "^6.4",
    "symfony/config": "^7.3",
    "symfony/yaml": "^7.3",
    "symfony/cache": "^7.3",
    "knplabs/github-api": "^3.15",
    "php-http/guzzle7-adapter": "^1.0",
    "league/commonmark": "^2.6",
    "doctrine/annotations": "^1.14",
    "symfony/twig-bundle": "^7.3",
    "twig/twig": "^3.14",
    "twig/markdown-extra": "^3.13",
    "twig/extra-bundle": "^3.13",
    "ext-zip": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^11.4",
    "phpstan/phpstan": "^2.1",
    "friendsofphp/php-cs-fixer": "^3.12",
    "squizlabs/php_codesniffer": "^3.8",
    "symfony/dotenv": "^7.3"
  },
  "scripts": {
    "analyse": "phpstan analyse src --level=max",
    "fix-style": "php-cs-fixer fix",
    "test": "phpunit",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  }
}
